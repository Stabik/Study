@model IEnumerable<ClassLibrary.model.SuperFolder>

@{
    ViewBag.Title = "LoadAllSuperFolder";
}

<h2>LoadAllSuperFolder</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SuperFolderName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <a href="/Document/LoadDocumentById/@item.Id">@Html.DisplayFor(modelItem => item.SuperFolderName)</a>
            
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
